{
    "name": "majframe/majframe",
    "description": "PHP MVC framework.",
    "type": "project",
    "minimum-stability": "stable",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ond≈ôej Maxa",
            "homepage": "http://ondrej.maxa.expert",
            "email": "ondrej@maxa.expert"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Unit\\": "tests/unit/",
            "App\\Functional\\": "tests/functional/",
            "App\\Acceptance\\": "tests/acceptance/"
        }
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "scripts": {
        "codecept": "codecept run",
        "codecept-steps": "@codecept --steps"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}